{
"manifest_version": 2,
"name": "JMT crawling app",
"description": "Tool for crawling JMT data ",
"version": "0.1",
"icons": {
    "128": "128.png"
},
"background": {
    "scripts":[
        "background.js"
    ],
    "persistent":true
},
"browser_action": {
    "default_title": "JML crawler",
    "default_popup" : "popup.html"
},
"permissions": [
    "contextMenus",
    "activeTab",
    "tabs",
    "<all_urls>"
]
}